DROP TABLE IF EXISTS tftdata;

CREATE TABLE tftdata (
  id BIGINT PRIMARY KEY,
  timestamp BIGINT NOT NULL,
  price FLOAT,
  quantity FLOAT,
  type_buysell INT,
  prev_timestamp BIGINT,
  next_timestamp BIGINT,
  prev_bid_1_price FLOAT, prev_bid_1_quantity FLOAT, prev_ask_1_price FLOAT, prev_ask_1_quantity FLOAT,
  next_bid_1_price FLOAT, next_bid_1_quantity FLOAT, next_ask_1_price FLOAT, next_ask_1_quantity FLOAT,
  prev_bid_2_price FLOAT, prev_bid_2_quantity FLOAT, prev_ask_2_price FLOAT, prev_ask_2_quantity FLOAT,
  next_bid_2_price FLOAT, next_bid_2_quantity FLOAT, next_ask_2_price FLOAT, next_ask_2_quantity FLOAT,
  prev_bid_3_price FLOAT, prev_bid_3_quantity FLOAT, prev_ask_3_price FLOAT, prev_ask_3_quantity FLOAT,
  next_bid_3_price FLOAT, next_bid_3_quantity FLOAT, next_ask_3_price FLOAT, next_ask_3_quantity FLOAT,
  prev_bid_4_price FLOAT, prev_bid_4_quantity FLOAT, prev_ask_4_price FLOAT, prev_ask_4_quantity FLOAT,
  next_bid_4_price FLOAT, next_bid_4_quantity FLOAT, next_ask_4_price FLOAT, next_ask_4_quantity FLOAT,
  prev_bid_5_price FLOAT, prev_bid_5_quantity FLOAT, prev_ask_5_price FLOAT, prev_ask_5_quantity FLOAT,
  next_bid_5_price FLOAT, next_bid_5_quantity FLOAT, next_ask_5_price FLOAT, next_ask_5_quantity FLOAT,
  prev_bid_6_price FLOAT, prev_bid_6_quantity FLOAT, prev_ask_6_price FLOAT, prev_ask_6_quantity FLOAT,
  next_bid_6_price FLOAT, next_bid_6_quantity FLOAT, next_ask_6_price FLOAT, next_ask_6_quantity FLOAT,
  prev_bid_7_price FLOAT, prev_bid_7_quantity FLOAT, prev_ask_7_price FLOAT, prev_ask_7_quantity FLOAT,
  next_bid_7_price FLOAT, next_bid_7_quantity FLOAT, next_ask_7_price FLOAT, next_ask_7_quantity FLOAT,
  prev_bid_8_price FLOAT, prev_bid_8_quantity FLOAT, prev_ask_8_price FLOAT, prev_ask_8_quantity FLOAT,
  next_bid_8_price FLOAT, next_bid_8_quantity FLOAT, next_ask_8_price FLOAT, next_ask_8_quantity FLOAT,
  prev_bid_9_price FLOAT, prev_bid_9_quantity FLOAT, prev_ask_9_price FLOAT, prev_ask_9_quantity FLOAT,
  next_bid_9_price FLOAT, next_bid_9_quantity FLOAT, next_ask_9_price FLOAT, next_ask_9_quantity FLOAT,
  prev_bid_10_price FLOAT, prev_bid_10_quantity FLOAT, prev_ask_10_price FLOAT, prev_ask_10_quantity FLOAT,
  next_bid_10_price FLOAT, next_bid_10_quantity FLOAT, next_ask_10_price FLOAT, next_ask_10_quantity FLOAT,
  prev_bid_11_price FLOAT, prev_bid_11_quantity FLOAT, prev_ask_11_price FLOAT, prev_ask_11_quantity FLOAT,
  next_bid_11_price FLOAT, next_bid_11_quantity FLOAT, next_ask_11_price FLOAT, next_ask_11_quantity FLOAT,
  prev_bid_12_price FLOAT, prev_bid_12_quantity FLOAT, prev_ask_12_price FLOAT, prev_ask_12_quantity FLOAT,
  next_bid_12_price FLOAT, next_bid_12_quantity FLOAT, next_ask_12_price FLOAT, next_ask_12_quantity FLOAT,
  prev_bid_13_price FLOAT, prev_bid_13_quantity FLOAT, prev_ask_13_price FLOAT, prev_ask_13_quantity FLOAT,
  next_bid_13_price FLOAT, next_bid_13_quantity FLOAT, next_ask_13_price FLOAT, next_ask_13_quantity FLOAT,
  prev_bid_14_price FLOAT, prev_bid_14_quantity FLOAT, prev_ask_14_price FLOAT, prev_ask_14_quantity FLOAT,
  next_bid_14_price FLOAT, next_bid_14_quantity FLOAT, next_ask_14_price FLOAT, next_ask_14_quantity FLOAT,
  prev_bid_15_price FLOAT, prev_bid_15_quantity FLOAT, prev_ask_15_price FLOAT, prev_ask_15_quantity FLOAT,
  next_bid_15_price FLOAT, next_bid_15_quantity FLOAT, next_ask_15_price FLOAT, next_ask_15_quantity FLOAT,
  prev_bid_16_price FLOAT, prev_bid_16_quantity FLOAT, prev_ask_16_price FLOAT, prev_ask_16_quantity FLOAT,
  next_bid_16_price FLOAT, next_bid_16_quantity FLOAT, next_ask_16_price FLOAT, next_ask_16_quantity FLOAT,
  prev_bid_17_price FLOAT, prev_bid_17_quantity FLOAT, prev_ask_17_price FLOAT, prev_ask_17_quantity FLOAT,

  prev_bid_18_price FLOAT, prev_bid_18_quantity FLOAT, prev_ask_18_price FLOAT, prev_ask_18_quantity FLOAT,
  next_bid_18_price FLOAT, next_bid_18_quantity FLOAT, next_ask_18_price FLOAT, next_ask_18_quantity FLOAT,
  prev_bid_19_price FLOAT, prev_bid_19_quantity FLOAT, prev_ask_19_price FLOAT, prev_ask_19_quantity FLOAT,
  next_bid_19_price FLOAT, next_bid_19_quantity FLOAT, next_ask_19_price FLOAT, next_ask_19_quantity FLOAT,
  prev_bid_20_price FLOAT, prev_bid_20_quantity FLOAT, prev_ask_20_price FLOAT, prev_ask_20_quantity FLOAT,
  next_bid_20_price FLOAT, next_bid_20_quantity FLOAT, next_ask_20_price FLOAT, next_ask_20_quantity FLOAT
);
